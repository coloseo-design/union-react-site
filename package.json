{
  "name": "union-design-site",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "demo": "npx gulp md",
    "inject": "node config-inject.js",
    "dev": "node config-inject.js env=development && yarn demo && export NODE_ENV=development &&  union-tool run dev",
    "build": "node config-inject.js env=development && export NODE_ENV=production && union-tool run build",
    "commit": "npx git-cz",
    "apidoc": "node ./scripts/apidoc.js",
    "init:commit": "npx commitizen init cz-conventional-changelog --yarn --dev --exact",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "dependencies": {
    "@types/classnames": "^2.2.11",
    "@types/react-dom": "^17.0.0",
    "algoliasearch": "^4.9.1",
    "classnames": "^2.2.6",
    "core-js": "^3.19.0",
    "dayjs": "^1.10.4",
    "enquire.js": "^2.1.6",
    "highlight-for-react": "^1.0.4",
    "omit.js": "^1.0.2",
    "react-highlight": "^0.14.0",
    "react-instantsearch-dom": "^6.11.0",
    "union-design": "^1.0.46-beta.2",
    "union-tool": "^0.1.2",
    "url-loader": "4.1.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@types/react-instantsearch-dom": "^6.10.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "commitizen": "^4.2.2",
    "conventional-changelog-cli": "^2.1.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "git-cz": "^4.7.0",
    "gulp-concat": "^2.6.1",
    "html-loader": "0.5.5",
    "mark-twain": "^2.0.3",
    "marked": "3.0.8",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-router-dom": "5.2.0",
    "single-line-log": "^1.1.2",
    "through2": "^4.0.2",
    "typescript": "^4.1.3",
    "webpack-bundle-analyzer": "^4.4.2"
